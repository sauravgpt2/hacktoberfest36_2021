#include<bits/stdc++.h>

using namespace std;

class Box

{

	private:		int l,b,h; 

	public:

		void setData(int length,int breadth,int height)

		{     

			l=length;    

			b=breadth;     

			h=height; 

		} 

		void showData() 

		{     

			cout<<"length = "<<l<<endl<<"breadth = "<<b<<endl<<"height = "<<h<<endl;  

		}

};

int main()

{    

	Box *p,smallBox;

	p=&smallBox;    

	p->setData(10,20,30); 

	//-> is used for pointer and . is used for objects  

	//smallBox.setData(10,20,30);  

	//smallBox.showData();    

	p->showData();    

return 0;

}

